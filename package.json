{
  "type": "module",
  "devDependencies": {
    "ava": "4.3.1",
    "chai": "4.3.6",
    "eslint": "8.20.0",
    "eslint-config-google": "0.14.0",
    "mocha": "10.0.0",
    "puppeteer": "15.5.0",
    "static-server": "2.2.1"
  },
  "private": true,
  "scripts": {
    "ava": "npx ava ./test/*.js",
    "dev": "npm run dev-frontend & npm run dev-backend",
    "dev-frontend": "npx http-server ./frontend/ -a=localhost --port=8080 -o /?environment=dev",
    "dev-backend": "PORT=8081 ACCESS_CONTROL=http://localhost:8080 npx nodemon ./server.js",
    "lint": "eslint '.'",
    "start": "node ./server/server.js",
    "test": "npm run lint && npm run ava"
  },
  "dependencies": {
    "express": "^4.18.1"
  }
}
