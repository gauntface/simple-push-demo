{
	"extends": ["eslint:recommended", "google"],
	"parserOptions": {
		"sourceType": "module"
	},
	"env": {
		"node": true,
		"es2021": true
	},
	"rules": {
		"no-var": 2,
		"require-jsdoc": 0,
		"no-console": 0,
    "indent": ["error", "tab"],
		"no-tabs": "off",
		"object-curly-spacing": ["error", "always"]
	}
}
